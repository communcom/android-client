<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RoundedView">
        <attr name="topLeftCornerRadius" format="dimension" />
        <attr name="topRightCornerRadius" format="dimension" />
        <attr name="bottomLeftCornerRadius" format="dimension" />
        <attr name="bottomRightCornerRadius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ButtonWithHintWidget">
        <attr name="button_with_hint_icon" format="reference" />

        <attr name="button_with_hint_icon_scale" format="float" />

        <attr name="button_with_hint_icon_tint" format="reference" />
        <attr name="button_with_hint_background" format="reference" />

        <attr name="button_with_hint_primary_text" format="string" />
        <attr name="button_with_hint_secondary_text" format="string" />
    </declare-styleable>

    <declare-styleable name="PinDigit">
        <attr name="state_empty" format="boolean" />
        <attr name="state_fill" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ImageButtonFixedIconSize">
        <attr name="imageButton_icon" format="reference" />

        <attr name="imageButton_iconWidth" format="dimension" />
        <attr name="imageButton_iconHeight" format="dimension" />

        <attr name="imageButton_iconInscribeSize" format="dimension" />

        <attr name="imageButton_tint" format="reference" />
    </declare-styleable>

    <declare-styleable name="SelectCommunityDialogList">
        <attr name="has_paging" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CollapsedPanelCoordinatorLayout">
        <attr name="toolbar" format="reference" />
        <attr name="toolbar_content" format="reference" />
        <attr name="collapsing_toolbar" format="reference" />
        <attr name="appbar" format="reference" />
        <attr name="toolbar_height" format="dimension" />
    </declare-styleable>

    <declare-styleable name="AvailableHoldView">
        <attr name="outer_color" format="color" />
        <attr name="inner_color_start" format="color" />
        <attr name="inner_color_end" format="color" />
        <attr name="stroke_width" format="dimension"/>
    </declare-styleable>
</resources>